  
<% var linkArray=[
  {'link':'#about','name':'ABOUT',},
  {'link':'#skill','name':'SKILL',},
  {'link':'#works','name':'WORKS',},
  {'link':'#contact','name':'CONTACT',},
];%>

  
<% var snsArray=[
  {'link':'#about','name':'ABOUT',},
  {'link':'#skill','name':'SKILL',},
  {'link':'#works','name':'WORKS',},
  {'link':'#contact','name':'CONTACT',},
];%>


<header class="l-header">
  <h1 class="l-header__description jsc-scroll-hide">
    河北一也のポートフォリオ
  </h1>
  <div class="l-header__inner">
    <a href="#" class="l-header__logo">
      <img
        src="assets/image/logo.png"
        alt="河北ポートフォリオロゴ"
      />
    </a>

    <div class="l-header__navWrap">
      <img
        src="assets/image/icon-hamburger.png"
        class="
          l-header__hamburger l-header__hamburger--open
          jsc-toggle-button-main
        "
        alt="メニューボタン"
      />
      <img
        src="assets/image/icon-close.png"
        class="
          l-header__hamburger l-header__hamburger--close
          jsc-toggle-button-main
        "
        alt="メニューを閉じる"
      />

      <nav class="l-header__nav jsc-nav-wrap-main">
        <ul class="l-header__list">

    <% for (var key in array) {%>
           <li class="l-header__listItem">
            <a href="<%= linkArray[key].link %>" class="l-header__listLink"><%= linkArray[key].name %></a>
          </li>
    <% } %>

        </ul>
      </nav>
    </div>

        <ul class="l-header__sns">
          <li class="l-header__snsItem">
            <a href="https://www.youtube.com/">
              <i class="fab fa-youtube"></i>
            </a>
          </li>

          <li class="l-header__snsItem">
            <a href="https://www.instagram.com/">
              <i class="fab fa-instagram"></i>
            </a>
          </li>

          <li class="l-header__snsItem">
            <a href="https://www.facebook.com/">
              <i class="fab fa-facebook-f"></i>
            </a>
          </li>

          <li class="l-header__snsItem">
            <a href="https://twitter.com/">
              <i class="fab fa-twitter"></i>
            </a>
          </li>
        </ul>
        <!-- l-header__sns -->
  </div>
  <!-- l-header__inner -->
</header>
