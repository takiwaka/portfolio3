<% var array=[
  {'name':'Marriage','text':'のんびり今を生きています。','modalImage':'modal-01',},
  {'name':'Kazuya','text':'のんびり今を生きています。','modalImage':'modal-01',},
  {'name':'Kawakita','text':'のんびり今を生きています。','modalImage':'modal-01',},
];%>

<div class="p-modal">
  <ul class="p-modal__contents u-mb-0 slick-modal slick-slider">
    <% for (var key in array) {%>
    <li class="p-modal__list">
      <div class="p-modal__imageBox">
        <img class="p-modal__image jsc-modal"
          src="assets/image/<%= array[key].modalImage %>.jpg" alt="">
      </div>
      <div class="p-modal__textBox">
        <p class="p-modal__name">
          <span class="p-modal__character jsc-modal">
            <%= array[key].name %>
          </span>
        </p>
      </div>
    </li>
    <% } %>
  </ul>
</div>
